<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="generated.entity.Account" table="acct" schema="hibernate_native2">
        <id name="acctNum">
            <column name="acct_num" sql-type="bigint(20)"/>
        </id>
        <property name="ballance">
            <column name="ballance" sql-type="double" precision="-1"/>
        </property>
        <property name="firstName">
            <column name="first_name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="lastName">
            <column name="last_name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="middleName">
            <column name="middle_name" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
